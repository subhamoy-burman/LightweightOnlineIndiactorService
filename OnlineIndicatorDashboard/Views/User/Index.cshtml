@model List<OnlineIndicatorDashboard.Models.User>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Last Heartbeat Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.LastHeartbeatTime</td>
            </tr>
        }
    </tbody>
</table>

